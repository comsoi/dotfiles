typeset -g -A key

key=(
    F1                        '^[OP'
    F2                        '^[OQ'
    F3                        '^[OR'
    F4                        '^[OS'
    F5                        '^[[15~'
    F6                        '^[[17~'
    F7                        '^[[18~'
    F8                        '^[[19~'
    F9                        '^[[20~'
    F10                       '^[[21~'
    F11                       '^[[23~'
    F12                       '^[[24~'
    Backspace                 '^?'
    Insert                    '^[[2~'
    Home                      '^[[H'
    PageUp                    '^[[5~'
    Delete                    '^[[3~'
    End                       '^[[F'
    PageDown                  '^[[6~'
    Up                        '^[[A'
    Left                      '^[[D'
    Down                      '^[[B'
    Right                     '^[[C'
    # Ctrl-Tab
    Ctrl-Backspace            '^H'
